<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gw.swipebacksample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.CommonActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
        <activity
            android:name=".activity.WxCommonActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
        <activity
            android:name=".activity.CommonAttachToActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
        <activity
            android:name=".activity.ScrollViewActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
        <activity
            android:name=".activity.HorizontalScrollViewActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
        <activity
            android:name=".activity.NestedScrollViewActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
        <activity
            android:name=".activity.ListViewActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
        <activity
            android:name=".activity.RecyclerViewActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
        <activity
            android:name=".activity.ViewPagerActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
        <activity
            android:name=".activity.WebViewActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
        <activity
            android:name=".activity.SwipeRefreshLayoutActivity"
            android:theme="@style/Theme.Swipe.Back.NoActionBar" />
    </application>

</manifest>